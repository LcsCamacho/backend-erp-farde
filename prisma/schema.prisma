// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  cpf      String @unique
  password String
}

model Funcionario {
  id                String  @id @default(cuid())
  nome              String
  sac               String
  cidade            String
  pdv               String
  processoConcluido Boolean
}
